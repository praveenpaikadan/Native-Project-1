{
  "expo": {
    "name": "AbooThahir",
    "slug": "AbooThahir",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/at-icon.png",
    "splash": {
      "image": "./assets/at-splash-screen.png",
      "resizeMode": "contain",
      "backgroundColor": "#FF4C00"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/at-icon.png",
        "backgroundColor": "#FF4C00"
      },
      "package": "testbuild.apk"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
